{
  "name": "PaackTask",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:android": "cd android/app && rm -rf build && cd .. && ./gradlew clean",
    "clean:pods": "cd ios && rm -rf pods && pod install",
    "clean:ios:build": "cd ios && rm -rf build",
    "clean:ios": "npm run clean:pods && npm run clean:ios:build",
    "tsc": "tsc",
    "build": "npm run tsc",
    "clean:modules": "rm -rf node_modules && yarn",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "bundle:android:release": "cd android && ./gradlew bundleRelease",
    "build:js:android": "react-native bundle --entry-file='index.js' --bundle-output='./android/app/src/main/assets/index.android.bundle' --dev=false --platform='android' --assets-dest='./android/app/src/main/res'",
    "build:js:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/PaackTask/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "test": "jest --config jest.config.js",
    "install:pods": "cd ios && pod install",
    "format_all": "prettier --write \"{__tests__,src}/**/*.{tsx,ts,json,js,jsx}\"",
    "tslint": "tslint -p ./tsconfig.json",
    "lint": "eslint . --config ./.eslintrc.json --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@mauron85/react-native-background-geolocation": "^0.6.3",
    "@react-native-async-storage/async-storage": "^1.13.3",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.1",
    "axios": "^0.21.1",
    "buffer": "^6.0.3",
    "lodash": "^4.17.20",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-background-timer": "^2.4.1",
    "react-native-device-info": "^8.0.1",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-permissions": "^3.0.0",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.15.2",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.10.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.5.3",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
